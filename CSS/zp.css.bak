  ul,ol,li,p,h1,h2,h3,h4,h5,h6,dl,dt,dd{
	margin:0px;
	padding:0px;
	list-style:none;
}
input{
	border:none;
}
	
		 header{
     width:100%;
     height:70px;
	 position:fixed;
	 z-index:10;
	 color:#fff;
     transition:all 1s linear;
  }
      header>ul{
	      list-style:none;
	  }

      header>ul li{
	     float:left;
		 text-align:center;
		 line-height:66px;
		 margin-left:100px;
	  }
	  header>ul li:hover{
	   
	  }
	   @-webkit-keyframes imgs1_2{
            0%{transform:rotateY(0deg);}
		    100%{transform:rotateY(360deg);}
		}
	   @-webkit-keyframes imgs2_2{
            0%{transform:rotateY(72deg);}
		    100%{transform:rotateY(432deg);}
       }
	   @-webkit-keyframes imgs3_2{
            0%{transform:rotateY(144deg);}
		    100%{transform:rotateY(504deg);}
       }
	   @-webkit-keyframes imgs4_2{
            0%{transform:rotateY(216deg);}
		    100%{transform:rotateY(576deg);}
       }
	   @-webkit-keyframes imgs5_2{
            0%{transform:rotateY(288deg);}
		    100%{transform:rotateY(648deg);}
       }
	    @-webkit-keyframes imgs1{
            0%{transform:rotateY(360deg);opacity:0.25}
			25%{opacity:0;}
			75%{opacity:1;}
			80%{opacity:0.3;}
		    100%{transform:rotateY(0deg);opacity:0.25;}
       }
	    @-webkit-keyframes imgs2{
            0%{transform:rotateY(-72deg);opacity:0.1;}
			5%{opacity:0;}
			55%{opacity:1;}
			60%{opacity:0.3;}
		    100%{transform:rotateY(-432deg);opacity:0.1;}
       }
	    @-webkit-keyframes imgs3{
            0%{transform:rotateY(-144deg);opacity:0.2;}
			35%{opacity:1;}
			40%{opacity:0.3;}
			85%{opacity:0;}
		    100%{transform:rotateY(-504deg);}
       }
	    @-webkit-keyframes imgs4{
            0%{transform:rotateY(504deg);opacity:0.7;}
			15%{opacity:1;}
			20%{opacity:0.3;}
			60%{opacity:0;}
		    100%{transform:rotateY(144deg);opacity:0.7;}
       }
	    @-webkit-keyframes imgs5{
            0%{transform:rotateY(432deg);opacity:0.3;}
			45%{opacity:0;}
			95%{opacity:1;}
		    100%{transform:rotateY(72deg);opacity:0.3;}
       }
	#cal_img1{-webkit-animation:imgs1_2 10s linear  infinite;}
	#cal_img2{-webkit-animation:imgs2_2 10s linear  infinite;}
	#cal_img3{-webkit-animation:imgs3_2 10s linear  infinite;}
	#cal_img4{-webkit-animation:imgs4_2 10s linear  infinite;}
	#cal_img5{-webkit-animation:imgs5_2 10s linear  infinite;}
	.cal1{-webkit-animation:imgs1 10s linear  infinite;}
	.cal2{-webkit-animation:imgs2 10s linear  infinite;}
	.cal3{-webkit-animation:imgs3 10s linear  infinite;}
	.cal4{-webkit-animation:imgs4 10s linear  infinite;}
	.cal5{-webkit-animation:imgs5 10s linear  infinite;}
    .cal{width:100%;background-color:#6a6;}
	.cal_box{transform-style:preserve-3d;transform:rotatex(-20deg) rotatey(0deg);}
	#cal_img1,#cal_img2,#cal_img3,#cal_img4,#cal_img5{width:20%;height:20%;transform-style:preserve-3d;}   
	img{width:100%;height:100%;border-radius:10%;box-shadow:1px 3px 1px black;}  
	 #cal{
	      -webkit-perspective:1200px;
		 
	     }
	
	#hhh{
	  width:100%;
	  height:5000px;
      border:1px solid #ddd;
	}
